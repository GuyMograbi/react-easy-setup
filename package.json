{
  "name": "react-easy-setup",
  "version": "0.0.0",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "connect-history-api-fallback": "^1.3.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "react-tools": "^0.13.3",
    "standard": "^8.6.0"
